        "bar": "bg-green-500",
        "text": "text-green-500",
        "button": "bg-green-100 text-green-700 hover:bg-green-200"
      },
      "warning": {
        "bar": "bg-yellow-500",
        "text": "text-yellow-500",
        "button": "bg-yellow-100 text-yellow-700 hover:bg-yellow-200"
      },
      "loading": {
